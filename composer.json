{
  "name": "lifeeka/jsql",
  "type": "library",
  "license": "MIT",
  "description": "Convert Json Object to SQL.",
  "keywords": ["json", "sql", "mysql", "convert", "laravel"],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "illuminate/database": ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "nunomaduro/collision": "^2.0",
    "mdurrant/php-console-color": "^1.0"

  },
  "autoload": {
    "psr-4": {
      "lifeeka\\jsql\\": "src"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\jsql\\": "tests"
    }
  }
}
